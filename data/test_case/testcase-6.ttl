[
    a ex:PolicyDemo;
    dct:title "Hard conflict detected. ";
    dct:desciption "Permission vs Prohibition in collection and partOf."
].
    
<> a ex:TestCase, ex:ConflictTestCase;
    dct:title "Hard conflict detected. ";
    report:policy <./data/policies/policy-6a.ttl>, <./data/policies/policy-6b.ttl>;
    ex:expectedReport [
        a report:PolicyReport, report:ConflictPolicyReport;
        report:ruleReport [
            a report:Report, report:ConflictReport;
